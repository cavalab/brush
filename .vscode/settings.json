{
    "cmake.cmakePath": "${env:CONDA_PREFIX}/bin/cmake",
    "cmake.buildBeforeRun": true,
    "cmake.buildTask": false,
    "cmake.buildDirectory": "${workspaceFolder}/build",
    "cmake.configureEnvironment": {
        "EIGEN3_INCLUDE_DIR": "${env:CONDA_PREFIX}/include/eigen3/",
        "GTEST_INCLUDE_DIRS": "${env:CONDA_PREFIX}/include/",
        "GTEST_LIBRARIES": "${env:CONDA_PREFIX}/lib/libgtest.so",
        "pybind11_DIR": "${env:CONDA_PREFIX}/lib/python3.8/site-packages/pybind11/share/cmake/pybind11/",
        "CONDA_LIB": "${env:CONDA_PREFIX}/lib/"
    },
    "files.associations": {
        "*.json.liquid": "json",
        "*.yaml.liquid": "yaml",
        "*.md.liquid": "markdown",
        "*.js.liquid": "liquid-javascript",
        "*.css.liquid": "liquid-css",
        "*.scss.liquid": "liquid-scss",
        "type_traits": "cpp",
        "string": "cpp",
        "cmath": "cpp",
        "array": "cpp",
        "atomic": "cpp",
        "bit": "cpp",
        "*.tcc": "cpp",
        "cctype": "cpp",
        "chrono": "cpp",
        "clocale": "cpp",
        "complex": "cpp",
        "cstddef": "cpp",
        "cstdint": "cpp",
        "cstdio": "cpp",
        "cstdlib": "cpp",
        "cstring": "cpp",
        "ctime": "cpp",
        "cwchar": "cpp",
        "cwctype": "cpp",
        "deque": "cpp",
        "forward_list": "cpp",
        "map": "cpp",
        "set": "cpp",
        "unordered_map": "cpp",
        "vector": "cpp",
        "exception": "cpp",
        "fstream": "cpp",
        "functional": "cpp",
        "initializer_list": "cpp",
        "iosfwd": "cpp",
        "iostream": "cpp",
        "istream": "cpp",
        "limits": "cpp",
        "memory": "cpp",
        "new": "cpp",
        "numeric": "cpp",
        "optional": "cpp",
        "ostream": "cpp",
        "ratio": "cpp",
        "sstream": "cpp",
        "stdexcept": "cpp",
        "streambuf": "cpp",
        "string_view": "cpp",
        "system_error": "cpp",
        "thread": "cpp",
        "tuple": "cpp",
        "typeindex": "cpp",
        "typeinfo": "cpp",
        "utility": "cpp",
        "valarray": "cpp",
        "variant": "cpp",
        "bitset": "cpp",
        "iomanip": "cpp",
        "ranges": "cpp",
        "stop_token": "cpp",
        "jacobi": "cpp",
        "random": "cpp",
        "mutex": "cpp",
        "__bit_reference": "cpp",
        "__node_handle": "cpp",
        "locale": "cpp"
    },
    "python.testing.pytestArgs": [
        "tests"
    ],
    "python.testing.unittestEnabled": false,
    "python.testing.pytestEnabled": true,
}
